@model IEnumerable<IdentityApp.Models.BuisnessModels.Ticket>

@{
    ViewBag.Title = "Index For Users";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CustomerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PassportId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfSeat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.CustomerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PassportId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberOfSeat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>



        </tr>
    }

</table>