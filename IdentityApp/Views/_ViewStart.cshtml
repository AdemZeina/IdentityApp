@{
    if (User.IsInRole("admin"))
    {
        Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
    }
    else
    {
        if (User.IsInRole("user"))
        {
            Layout = "~/Views/Shared/_LayoutUser.cshtml";
        }
        else
        {
            Layout = "~/Views/Shared/_Layout.cshtml";
        }

    }

}
